    <search-param>
      <name>id</name>
      <field>ID</field>
      <operator>=</operator>
      <container>
        <label>Id</label>
        <type>text</type>
        <size>128</size>
	<validation>
	  <converter>number</converter>
	  <min-value>1</min-value>
	  <max-value>10</max-value>
	</validation>
      </container>
    </search-param>
    
    <search-param>
      <name>name</name>
      <field>NAME</field>
      <operator>=</operator>
      <container>
        <label>Name</label>
        <type>text</type>
        <size>128</size>
	<validation>
	  <converter>date</converter>
	  <converter-properties>
	    <property>
	      <name>pt</name>
	      <value>MM</value>
	    </property>
	    <property>
	      <name>en</name>
	      <value>MM</value>
	    </property>
	  </converter-properties>
	</validation>
      </container>
    </search-param>
